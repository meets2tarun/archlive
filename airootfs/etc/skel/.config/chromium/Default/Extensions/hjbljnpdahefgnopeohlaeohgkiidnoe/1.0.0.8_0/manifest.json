{
   "app": {
      "background": {
         "scripts": [ "common/jquery-2.1.1.min.js", "common/controller.js", "uglify.js" ]
      }
   },
   "description": "Receive videos from AllCast for Android.",
   "icons": {
      "128": "common/icon.png",
      "16": "common/icon.png",
      "48": "common/icon.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "AllCast Receiver",
   "permissions": [ "usb", {
      "usbDevices": [ {
         "productId": 20194,
